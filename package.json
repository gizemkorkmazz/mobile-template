{
  "name": "mobileTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build-apk": "cd android && ./gradlew assembleRelease",
    "build-bundle": "cd android && ./gradlew bundleRelease",
    "install-apk": "react-native run-android --variant=release",
    "uninstall-apk": "adb uninstall com.omegabigdata.maxion",
    "run-all-prod": "yarn build-apk && yarn uninstall-apk && yarn install-apk",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-x": "react-native run-ios --simulator='iPhone X'",
    "ios-xr": "react-native run-ios --simulator='iPhone XÊ€'",
    "ios-xs-max": "react-native run-ios --simulator='iPhone Xs Max'",
    "ios-6": "react-native run-ios --simulator='iPhone 6'",
    "ios-6p": "react-native run-ios --simulator='iPhone 6 Plus'",
    "ios-7": "react-native run-ios --simulator='iPhone 7'",
    "ios-7p": "react-native run-ios --simulator='iPhone 7 Plus'",
    "ios-8": "react-native run-ios --simulator='iPhone 8'",
    "ios-8p": "react-native run-ios --simulator='iPhone 8 Plus'",
    "clean-android": "cd android && ./gradlew clean",
    "reset-cache": "yarn start --reset-cache",
    "clean-cache": "watchman watch-del-all && rm -rf node_modules && yarn && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*"
  },
  "dependencies": {
    "@crowdlinker/react-native-pager": "^0.2.1",
    "@omegabigdata/apollo-rn-redux-helper": "^1.5.1",
    "react": "16.9.0",
    "react-native": "0.61.2",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-onesignal": "^3.4.2",
    "react-native-reanimated": "^1.3.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-animated-switch": "^0.3.2",
    "react-navigation-drawer": "^2.3.1",
    "react-navigation-stack": "^1.9.4",
    "react-redux": "^7.1.1",
    "reactotron-react-native": "^4.0.2",
    "reactotron-redux": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
